{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "additionalProperties": false,
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Software Engineer"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                10000
            ]
        },
        "equity": {
            "type": "number",
            "default": 0.0,
            "maximum" : 1.0,
            "title": "The equity Schema",
            "examples": [
                0.765
            ]
        }
    },
    "examples": [{
        "title": "Software Engineer",
        "salary": 10000,
        "equity": 0.765
    }]
}